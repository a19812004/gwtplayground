<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  
    <servlet>
    <servlet-name>logAndPassServlet</servlet-name>
    <servlet-class>partShop.server.LogAndPassImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>logAndPassServlet</servlet-name>
    <url-pattern>/partShop/logAndPass</url-pattern>
  </servlet-mapping>
  
      <servlet>
    <servlet-name>AnOrderFormServlet</servlet-name>
    <servlet-class>partShop.server.AnOrderFormImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>AnOrderFormServlet</servlet-name>
    <url-pattern>/partShop/AnOrderForm</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>PartShop.html</welcome-file>
  </welcome-file-list>

</web-app>
